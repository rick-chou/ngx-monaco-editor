{
  "name": "@rickzhou/ngx-monaco-editor",
  "version": "14.0.0",
  "description": "Monaco Code Editor For Angular",
  "keywords": [
    "angular",
    "monaco",
    "monaco-editor",
    "code",
    "editor",
    "reusable"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "run-s build-editor serve",
    "serve": "ng serve",
    "build": "ng build --project=editor-tester --base-href=editor-tester --configuration=development",
    "build:prod": "ng build --project=editor-tester --base-href=editor-tester",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-editor": "npm run copy:readme && ng build editor",
    "npm-publish": "npm run build-editor && bash npm-publish",
    "copy:readme": "cp -rf README.md projects/editor/"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/rick-chou/ngx-monaco-editor"
  },
  "bugs": {
    "url": "https://github.com/rick-chou/ngx-monaco-editor/issues"
  },
  "contributors": [
    "Atul Kumar <atulaggarwal4288@gmail.com>",
    "Rick Zhou <rickzhou209@gmail.com>"
  ],
  "dependencies": {
    "@angular/animations": "~14.2.2",
    "@angular/common": "~14.2.2",
    "@angular/compiler": "~14.2.2",
    "@angular/core": "~14.2.2",
    "@angular/forms": "~14.2.2",
    "@angular/platform-browser": "~14.2.2",
    "@angular/platform-browser-dynamic": "~14.2.2",
    "@angular/router": "~14.2.2",
    "monaco-editor": "^0.34.0",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.3",
    "@angular/cli": "~14.2.3",
    "@angular/compiler-cli": "~14.2.2",
    "@angular/language-service": "~14.2.2",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.7.18",
    "jasmine-core": "~4.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^14.2.1",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.8.3"
  }
}
